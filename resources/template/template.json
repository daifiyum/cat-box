{
  "log": {
    "level": "info",
    "timestamp": false
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "path": "resources/db/cache.db"
    },
    "clash_api": {
      "default_mode": "rule",
      "external_controller": "0.0.0.0:9090",
      "external_ui": "resources/ui/clash"
    }
  },
  "dns": {
    "servers": [
      {
        "tag": "remote",
        "address": "https://1.1.1.1/dns-query",
        "address_strategy": "ipv4_only",
        "strategy": "prefer_ipv4",
        "detour": "proxy"
      },
      {
        "tag": "local",
        "address": "https://223.6.6.6/dns-query",
        "address_strategy": "ipv4_only",
        "strategy": "prefer_ipv4",
        "detour": "direct"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "action": "route",
        "server": "local"
      },
      {
        "rule_set": "geosite-cn",
        "action": "route",
        "server": "local"
      },
      {
        "clash_mode": "global",
        "action": "route",
        "server": "remote"
      }
    ],
    "final": "remote"
  },
  "inbounds": [
    {
      "listen": "::",
      "listen_port": 8888,
      "set_system_proxy": false,
      "tag": "mixed-in",
      "type": "mixed"
    },
    {
      "type": "tun",
      "stack": "mixed",
      "address": ["172.19.0.1/30", "fdfe:dcba:9876::0/126"],
      "mtu": 9000,
      "auto_route": true,
      "strict_route": true
    }
  ],
  "outbounds": [
    {
      "tag": "proxy",
      "type": "selector",
      "outbounds": ["auto", "direct"]
    },
    {
      "tag": "openai",
      "type": "selector",
      "outbounds": []
    },
    {
      "tag": "auto",
      "type": "urltest",
      "outbounds": [],
      "url": "http://www.gstatic.com/generate_204",
      "interval": "10m",
      "tolerance": 50
    },
    {
      "type": "direct",
      "tag": "direct"
    }
  ],
  "route": {
    "rules": [
      {
        "action": "sniff"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },
      {
        "protocol": "quic",
        "action": "reject",
        "method": "drop"
      },
      {
        "clash_mode": "global",
        "action": "route",
        "outbound": "proxy"
      },
      {
        "rule_set": "ad",
        "action": "reject",
        "method": "drop"
      },
      {
        "rule_set": "geosite-cn",
        "action": "route",
        "outbound": "direct"
      },
      {
        "rule_set": "geoip-cn",
        "action": "route",
        "outbound": "direct"
      },
      {
        "rule_set": "openai",
        "action": "route",
        "outbound": "openai"
      },
      {
        "ip_is_private": true,
        "action": "route",
        "outbound": "direct"
      },
      {
        "domain_suffix": [
          "gpt.ge",
          "iplaysoft.com",
          "appinn.com",
          "deepseek.com"
        ],
        "action": "route",
        "outbound": "direct"
      }
    ],
    "rule_set": [
      {
        "tag": "geoip-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geoip/cn.srs",
        "download_detour": "direct"
      },
      {
        "tag": "geosite-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/cn.srs",
        "download_detour": "direct"
      },
      {
        "tag": "openai",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/Toperlock/sing-box-geosite/main/rule/OpenAI.srs",
        "download_detour": "proxy"
      },
      {
        "tag": "ad",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/REIJI007/AdBlock_Rule_For_Sing-box/main/adblock_reject.srs",
        "download_detour": "proxy"
      }
    ],
    "auto_detect_interface": true,
    "final": "proxy"
  }
}
